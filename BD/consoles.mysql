CREATE DATABASE erp_networking;
USE erp_networking;

CREATE TABLE CONSOLE_DEV(
	id int(5) NOT NULL AUTO_INCREMENT,
    date DATETIME,
    message varchar(350),
    PRIMARY KEY (id)
);

CREATE TABLE CONSOLE_ADMIN(
	id int(5) NOT NULL AUTO_INCREMENT,
    date DATETIME,
    message varchar(350),
    PRIMARY KEY (id)
);